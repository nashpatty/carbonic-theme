{
	"$schema": "vscode://schemas/color-theme",
	"name": "Carbonic Light",
	"colors": {
		
		"editor.background": "#f4f4f4",
        "editor.foreground": "#161616",
        "editor.selectionBackground": "#bae6ff",
		"editorWhitespace.foreground": "#a8a8a8",
		"editor.lineHighlightBorder": "#e0e0e0",
        "editorLineNumber.foreground": "#a8a8a8",
        "editorLineNumber.activeForeground": "#393939",
        "editorBracketMatch.background": "#c6c6c6",
        "editorBracketMatch.border": "#c6c6c6",
        "editorBracketHighlight.unexpectedBracket.foreground": "#750e13",
        "editor.wordHighlightBackground": "#e0e0e0E6",
        "editor.wordHighlightStrongBackground": "#002d9c80",
        "editorIndentGuide.background1": "#e0e0e0CC",
        "editorIndentGuide.activeBackground1": "#c6c6c6",
        "editorRuler.foreground": "#e0e0e0",
        "editor.findMatchBackground": "#ffd9be",
        "editor.findMatchHighlightBackground": "#a7f0baB3",

        // terminal colors
        "terminal.ansiBlack": "#e0e0e0",
        "terminal.ansiBlue": "#001d6c",
        "terminal.ansiBrightBlack": "#e0e0e0",
        "terminal.ansiBrightBlue": "#001d6c",
        "terminal.ansiBrightCyan": "#005d5d",
        "terminal.ansiBrightGreen": "#0e6027",
        "terminal.ansiBrightMagenta": "#491d8b",
        "terminal.ansiBrightRed": "#750e13",
        "terminal.ansiBrightWhite": "#161616",
        "terminal.ansiBrightYellow": "#483700",
        "terminal.ansiCyan": "#005d5d",
        "terminal.ansiGreen": "#0e6027",
        "terminal.ansiMagenta": "#491d8b",
        "terminal.ansiRed": "#750e13",
        "terminal.ansiWhite": "#161616",
        "terminal.ansiYellow": "#483700",
        "terminal.background": "#161616",
        "terminal.foreground": "#161616",
	},
	"tokenColors": [
        {
            "name": "keyword or other syntax",
            "scope": "punctuation.terminator,constant.character.escape,punctuation.quasi.element",
            "settings": {
                "foreground": "#001d6c"
            }
        },
        {
            "name": "no highlight",
            "scope": "variable.parameter,variable.other,string.unquoted.argument.shell,storage.type.object,storage.type.java,constant.other.option",
            "settings": {
                "foreground": "#161616"
            }
        },
		{
			"scope": "entity.name.function,support.function,support.type.property-name.json,keyword.control.twig",
			"settings": {
				"foreground": "#491d8b",
			}
		},
        {
            "scope": [
                "meta.embedded",
                "source.groovy.embedded",
                "string meta.image.inline.markdown",
                "variable.legacy.builtin.python"
            ],
            "settings": {
                "foreground": "#161616"
            }
        },
        {
            "scope": "emphasis",
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "scope": "strong",
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "name": "Comment",
            "scope": "comment",
            "settings": {
                "foreground": "#6f6f6f"
            }
        },
        {
            "name": "block comment",
            "scope": "comment.block",
            "settings": {
                "foreground": "#6f6f6f",
            }
        },
        {
            "scope": "constant.language",
            "settings": {
                "foreground": "#001d6c"
            }
        },
        {
            "scope": [
                "constant.numeric",
                "variable.other.enummember",
                "keyword.operator.plus.exponent",
                "keyword.operator.minus.exponent"
            ],
            "settings": {
                "foreground": "#005d5d"
            }
        },
        {
            "scope": "constant.regexp",
            "settings": {
                "foreground": "#001d6c"
            }
        },
        {
            "scope": "entity.name.tag",
            "settings": {
                "foreground": "#001d6c"
            }
        },
        {
            "scope": [
                "entity.name.tag.css",
                "entity.name.tag.less"
            ],
            "settings": {
                "foreground": "#001d6c"
            }
        },
        {
            "scope": "entity.other.attribute-name,constant.other",
            "settings": {
                "foreground": "#00539a"
            }
        },
        {
            "scope": [
                "entity.other.attribute-name.class.css",
                "source.css entity.other.attribute-name.class",
                "entity.other.attribute-name.id.css",
                "entity.other.attribute-name.parent-selector.css",
                "entity.other.attribute-name.parent.less",
                "source.css entity.other.attribute-name.pseudo-class",
                "entity.other.attribute-name.pseudo-element.css",
                "source.css.less entity.other.attribute-name.id",
                "entity.other.attribute-name.scss"
            ],
            "settings": {
                "foreground": "#001d6c"
            }
        },
        {
            "scope": "invalid",
            "settings": {
                "foreground": "#db4934"
            }
        },
        {
            "scope": "markup.underline",
            "settings": {
                "fontStyle": "underline"
            }
        },
        {
            "scope": "markup.bold",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#001d6c"
            }
        },
        {
            "scope": "markup.heading",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#001d6c"
            }
        },
        {
            "scope": "markup.italic",
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "scope": "markup.strikethrough",
            "settings": {
                "fontStyle": "strikethrough"
            }
        },
        {
            "scope": "markup.inserted",
            "settings": {
                "foreground": "#491d8b"
            }
        },
        {
            "scope": "markup.deleted",
            "settings": {
                "foreground": "#0e6027"
            }
        },
        {
            "scope": "markup.changed",
            "settings": {
                "foreground": "#001d6c"
            }
        },
        {
            "scope": "punctuation.definition.quote.begin.markdown",
            "settings": {
                "foreground": "#0e6027"
            }
        },
        {
            "scope": "punctuation.definition.list.begin.markdown",
            "settings": {
                "foreground": "#001d6c"
            }
        },
        {
            "scope": "markup.inline.raw",
            "settings": {
                "foreground": "#0e6027"
            }
        },
        {
            "name": "brackets of XML/HTML tags",
            "scope": "punctuation.definition.tag",
            "settings": {
                "foreground": "#393939"
            }
        },
        {
            "scope": [
                "meta.preprocessor",
                "entity.name.function.preprocessor"
            ],
            "settings": {
                "foreground": "#001d6c"
            }
        },
        {
            "scope": "meta.preprocessor.string",
            "settings": {
                "foreground": "#0e6027"
            }
        },
        {
            "scope": "meta.preprocessor.numeric",
            "settings": {
                "foreground": "#005d5d"
            }
        },
        {
            "scope": "meta.diff.header",
            "settings": {
                "foreground": "#001d6c"
            }
        },
        {
            "scope": "storage",
            "settings": {
                "foreground": "#001d6c"
            }
        },
        {
            "scope": "storage.type",
            "settings": {
                "foreground": "#001d6c"
            }
        },
        {
            "scope": [
                "storage.modifier",
                "keyword.operator.noexcept"
            ],
            "settings": {
                "foreground": "#001d6c"
            }
        },
        {
            "scope": [
                "string",
                "meta.embedded.assembly",
            ],
            "settings": {
                "foreground": "#0e6027"
            }
        },
        {
            "scope": "string.tag",
            "settings": {
                "foreground": "#0e6027"
            }
        },
        {
            "scope": "string.value",
            "settings": {
                "foreground": "#0e6027"
            }
        },
        {
            "scope": "string.regexp",
            "settings": {
                "foreground": "#0e6027"
            }
        },
        {
            "name": "String interpolation",
            "scope": [
                "punctuation.definition.template-expression.begin",
                "punctuation.definition.template-expression.end",
                "punctuation.section.embedded"
            ],
            "settings": {
                "foreground": "#001d6c"
            }
        },
        {
            "name": "Reset JavaScript string interpolation expression",
            "scope": [
                "meta.template.expression"
            ],
            "settings": {
                "foreground": "#161616"
            }
        },
        {
            "scope": [
                "support.type.vendored.property-name",
                "support.type.property-name",
                "source.css variable",
                "source.coffee.embedded"
            ],
            "settings": {
                "foreground": "#161616"
            }
        },
        {
            "scope": "keyword",
            "settings": {
                "foreground": "#001d6c"
            }
        },
        {
            "scope": "keyword.control",
            "settings": {
                "foreground": "#001d6c"
            }
        },
        {
            "scope": "keyword.operator",
            "settings": {
                "foreground": "#161616"
            }
        },
        {
            "scope": [
                "keyword.operator.new",
                "keyword.operator.expression",
                "keyword.operator.cast",
                "keyword.operator.sizeof",
                "keyword.operator.alignof",
                "keyword.operator.typeid",
                "keyword.operator.alignas",
                "keyword.operator.instanceof",
                "keyword.operator.logical.python",
                "keyword.operator.wordlike"
            ],
            "settings": {
                "foreground": "#001d6c"
            }
        },
        {
            "scope": "keyword.other.unit",
            "settings": {
                "foreground": "#005d5d"
            }
        },
        {
            "scope": [
                "punctuation.section.embedded.begin.php",
                "punctuation.section.embedded.end.php"
            ],
            "settings": {
                "foreground": "#001d6c"
            }
        },
        {
            "name": "coloring of the Java import and package identifiers",
            "scope": [
                "storage.modifier.import.java",
                "variable.language.wildcard.java",
                "storage.modifier.package.java"
            ],
            "settings": {
                "foreground": "#161616"
            }
        },
        {
            "name": "this.self",
            "scope": "variable.language",
            "settings": {
                "foreground": "#001d6c"
            }
        },
        {
            "name": "built in properties",
            "scope": [
                "support.constant.property-value",
                "keyword.control.at-rule",
            ],
            "settings": {
                "foreground": "#00539a"
            }
        }
    ],
    "semanticHighlighting": true,
    "semanticTokenColors": {
        "newOperator": "#161616",
        "stringLiteral": "#0e6027",
        "customLiteral": "#161616",
        "numberLiteral": "#005d5d"
    }
}
