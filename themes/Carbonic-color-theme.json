{
	"name": "Carbonic",
	"colors": {
		"editor.background": "#262626",
        "editor.foreground": "#e0e0e0",
        "editor.selectionBackground": "#003a6dd9",
		"editorWhitespace.foreground": "#525252",
		"editor.lineHighlightBorder": "#39393980",
        "editorLineNumber.foreground": "#525252",
        "editorLineNumber.activeForeground": "#a8a8a8",
        "editorBracketMatch.background": "#393939",
        "editorBracketMatch.border": "#525252",
        "editorBracketHighlight.unexpectedBracket.foreground": "#fa4d56",
        "editor.wordHighlightBackground": "#52525280",
        "editor.wordHighlightStrongBackground": "#002d9c80",
        "editorIndentGuide.background1": "#393939bf",
        "editorIndentGuide.activeBackground1": "#525252",
        "editorRuler.foreground": "#262626",
        "editor.findMatchBackground": "#750e13",
        "editor.findMatchHighlightBackground": "#044317d9",

        // terminal colors
        "terminal.ansiBlack": "#161616",
        "terminal.ansiBlue": "#78a9ff",
        "terminal.ansiBrightBlack": "#161616",
        "terminal.ansiBrightBlue": "#78a9ff",
        "terminal.ansiBrightCyan": "#3ddbd9",
        "terminal.ansiBrightGreen": "#6fdc8c",
        "terminal.ansiBrightMagenta": "#d4bbff",
        "terminal.ansiBrightRed": "#fa4d56",
        "terminal.ansiBrightWhite": "#f4f4f4",
        "terminal.ansiBrightYellow": "#f1c21b",
        "terminal.ansiCyan": "#3ddbd9",
        "terminal.ansiGreen": "#6fdc8c",
        "terminal.ansiMagenta": "#d4bbff",
        "terminal.ansiRed": "#EA4430",
        "terminal.ansiWhite": "#f4f4f4",
        "terminal.ansiYellow": "#f1c21b",
        "terminal.background": "#262626",
        "terminal.foreground": "#e0e0e0",
	},
	"tokenColors": [
        {
            "name": "keyword or other syntax",
            "scope": "punctuation.terminator,constant.character.escape,punctuation.quasi.element",
            "settings": {
                "foreground": "#78a9ff"
            }
        },
        {
            "name": "no highlight",
            "scope": "variable.parameter,variable.other,string.unquoted.argument.shell,storage.type.object,storage.type.java,constant.other.option",
            "settings": {
                "foreground": "#e0e0e0"
            }
        },
		{
			"scope": "entity.name.function,support.function,support.type.property-name.json,keyword.control.twig",
			"settings": {
				"foreground": "#d4bbff",
			}
		},
        {
            "scope": [
                "meta.embedded",
                "source.groovy.embedded",
                "string meta.image.inline.markdown",
                "variable.legacy.builtin.python"
            ],
            "settings": {
                "foreground": "#e0e0e0"
            }
        },
        {
            "scope": "emphasis",
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "scope": "strong",
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "name": "Comment",
            "scope": "comment",
            "settings": {
                "foreground": "#8d8d8d"
            }
        },
        {
            "name": "block comment",
            "scope": "comment.block",
            "settings": {
                "foreground": "#8d8d8d",
            }
        },
        {
            "scope": "constant.language",
            "settings": {
                "foreground": "#78a9ff"
            }
        },
        {
            "scope": [
                "constant.numeric",
                "variable.other.enummember",
                "keyword.operator.plus.exponent",
                "keyword.operator.minus.exponent"
            ],
            "settings": {
                "foreground": "#3ddbd9"
            }
        },
        {
            "scope": "constant.regexp",
            "settings": {
                "foreground": "#4589ff"
            }
        },
        {
            "scope": "entity.name.tag",
            "settings": {
                "foreground": "#78a9ff"
            }
        },
        {
            "scope": [
                "entity.name.tag.css",
                "entity.name.tag.less"
            ],
            "settings": {
                "foreground": "#78a9ff"
            }
        },
        {
            "scope": "entity.other.attribute-name",
            "settings": {
                "foreground": "#bae6ff"
            }
        },
        {
            "scope": [
                "entity.other.attribute-name.class.css",
                "source.css entity.other.attribute-name.class",
                "entity.other.attribute-name.id.css",
                "entity.other.attribute-name.parent-selector.css",
                "entity.other.attribute-name.parent.less",
                "source.css entity.other.attribute-name.pseudo-class",
                "entity.other.attribute-name.pseudo-element.css",
                "source.css.less entity.other.attribute-name.id",
                "entity.other.attribute-name.scss"
            ],
            "settings": {
                "foreground": "#78a9ff"
            }
        },
        {
            "scope": "invalid",
            "settings": {
                "foreground": "#db4934"
            }
        },
        {
            "scope": "markup.underline",
            "settings": {
                "fontStyle": "underline"
            }
        },
        {
            "scope": "markup.bold",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#78a9ff"
            }
        },
        {
            "scope": "markup.heading",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#78a9ff"
            }
        },
        {
            "scope": "markup.italic",
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "scope": "markup.strikethrough",
            "settings": {
                "fontStyle": "strikethrough"
            }
        },
        {
            "scope": "markup.inserted",
            "settings": {
                "foreground": "#d4bbff"
            }
        },
        {
            "scope": "markup.deleted",
            "settings": {
                "foreground": "#6fdc8c"
            }
        },
        {
            "scope": "markup.changed",
            "settings": {
                "foreground": "#78a9ff"
            }
        },
        {
            "scope": "punctuation.definition.quote.begin.markdown",
            "settings": {
                "foreground": "#6fdc8c"
            }
        },
        {
            "scope": "punctuation.definition.list.begin.markdown",
            "settings": {
                "foreground": "#78a9ff"
            }
        },
        {
            "scope": "markup.inline.raw",
            "settings": {
                "foreground": "#6fdc8c"
            }
        },
        {
            "name": "brackets of XML/HTML tags",
            "scope": "punctuation.definition.tag",
            "settings": {
                "foreground": "#a8a8a8"
            }
        },
        {
            "scope": [
                "meta.preprocessor",
                "entity.name.function.preprocessor"
            ],
            "settings": {
                "foreground": "#78a9ff"
            }
        },
        {
            "scope": "meta.preprocessor.string",
            "settings": {
                "foreground": "#6fdc8c"
            }
        },
        {
            "scope": "meta.preprocessor.numeric",
            "settings": {
                "foreground": "#3ddbd9"
            }
        },
        {
            "scope": "meta.diff.header",
            "settings": {
                "foreground": "#78a9ff"
            }
        },
        {
            "scope": "storage",
            "settings": {
                "foreground": "#78a9ff"
            }
        },
        {
            "scope": "storage.type",
            "settings": {
                "foreground": "#78a9ff"
            }
        },
        {
            "scope": [
                "storage.modifier",
                "keyword.operator.noexcept"
            ],
            "settings": {
                "foreground": "#78a9ff"
            }
        },
        {
            "scope": [
                "string",
                "meta.embedded.assembly",
            ],
            "settings": {
                "foreground": "#6fdc8c"
            }
        },
        {
            "scope": "string.tag",
            "settings": {
                "foreground": "#6fdc8c"
            }
        },
        {
            "scope": "string.value",
            "settings": {
                "foreground": "#6fdc8c"
            }
        },
        {
            "scope": "string.regexp",
            "settings": {
                "foreground": "#6fdc8c"
            }
        },
        {
            "name": "String interpolation",
            "scope": [
                "punctuation.definition.template-expression.begin",
                "punctuation.definition.template-expression.end",
                "punctuation.section.embedded"
            ],
            "settings": {
                "foreground": "#78a9ff"
            }
        },
        {
            "name": "Reset JavaScript string interpolation expression",
            "scope": [
                "meta.template.expression"
            ],
            "settings": {
                "foreground": "#e0e0e0"
            }
        },
        {
            "scope": [
                "support.type.vendored.property-name",
                "support.type.property-name",
                "source.css variable",
                "source.coffee.embedded"
            ],
            "settings": {
                "foreground": "#e0e0e0"
            }
        },
        {
            "scope": "keyword",
            "settings": {
                "foreground": "#78a9ff"
            }
        },
        {
            "scope": "keyword.control",
            "settings": {
                "foreground": "#78a9ff"
            }
        },
        {
            "scope": "keyword.operator",
            "settings": {
                "foreground": "#e0e0e0"
            }
        },
        {
            "scope": [
                "keyword.operator.new",
                "keyword.operator.expression",
                "keyword.operator.cast",
                "keyword.operator.sizeof",
                "keyword.operator.alignof",
                "keyword.operator.typeid",
                "keyword.operator.alignas",
                "keyword.operator.instanceof",
                "keyword.operator.logical.python",
                "keyword.operator.wordlike"
            ],
            "settings": {
                "foreground": "#78a9ff"
            }
        },
        {
            "scope": "keyword.other.unit",
            "settings": {
                "foreground": "#3ddbd9"
            }
        },
        {
            "scope": [
                "punctuation.section.embedded.begin.php",
                "punctuation.section.embedded.end.php"
            ],
            "settings": {
                "foreground": "#78a9ff"
            }
        },
        {
            "name": "coloring of the Java import and package identifiers",
            "scope": [
                "storage.modifier.import.java",
                "variable.language.wildcard.java",
                "storage.modifier.package.java"
            ],
            "settings": {
                "foreground": "#e0e0e0"
            }
        },
        {
            "name": "this.self",
            "scope": "variable.language",
            "settings": {
                "foreground": "#78a9ff"
            }
        },
        {
            "name": "built in properties",
            "scope": [
                "support.constant.property-value",
                "keyword.control.at-rule",
            ],
            "settings": {
                "foreground": "#bae6ff"
            }
        }
    ],
    "semanticHighlighting": true,
    "semanticTokenColors": {
        "newOperator": "#e0e0e0",
        "stringLiteral": "#6fdc8c",
        "customLiteral": "#e0e0e0",
        "numberLiteral": "#3ddbd9"
    }
}
