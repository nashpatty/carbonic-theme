{
	"name": "Carbonic",
	"colors": {
		"editor.background": "#262626",
        "editor.foreground": "#c6c6c6",
        "editor.selectionBackground": "#07667859",
		"editorWhitespace.foreground": "#504945",
        "editor.lineHighlightBackground": "#282828",
        "editorLineNumber.foreground": "#665c54",
        "editorLineNumber.activeForeground": "#a89984",
        "editorBracketMatch.background": "#504945",
        "editorBracketMatch.border": "#7c6f64",
        "editorBracketHighlight.unexpectedBracket.foreground": "#D1243B",
        "editor.wordHighlightBackground": "#32593d80",
        "editor.wordHighlightStrongBackground": "#532b2e80",
        "editorIndentGuide.background1": "#50494566",
        "editorIndentGuide.activeBackground1": "#504945",
        "editorRuler.foreground": "#3c3836",
        "editor.findMatchBackground": "#532b2e",
        "editor.findMatchHighlightBackground": "#d79a2140",

        // terminal colors
        "terminal.ansiBlack": "#161616",
        "terminal.ansiBlue": "#78a9ff",
        "terminal.ansiBrightBlack": "#161616",
        "terminal.ansiBrightBlue": "#78a9ff",
        "terminal.ansiBrightCyan": "#bae6ff",
        "terminal.ansiBrightGreen": "#6fdc8c",
        "terminal.ansiBrightMagenta": "#d4bbff",
        "terminal.ansiBrightRed": "#fa4d56",
        "terminal.ansiBrightWhite": "#c6c6c6",
        "terminal.ansiBrightYellow": "#d2a106",
        "terminal.ansiCyan": "#bae6ff",
        "terminal.ansiGreen": "#6fdc8c",
        "terminal.ansiMagenta": "#d4bbff",
        "terminal.ansiRed": "#EA4430",
        "terminal.ansiWhite": "#c6c6c6",
        "terminal.ansiYellow": "#d2a106",
        "terminal.background": "#1d2021",
        "terminal.foreground": "#c6c6c6",
	},
	"tokenColors": [
        {
            "name": "keyword or other syntax",
            "scope": "punctuation.terminator,constant.character.escape,punctuation.quasi.element",
            "settings": {
                "foreground": "#78a9ff"
            }
        },
        {
            "name": "no highlight",
            "scope": "variable.parameter,variable.other,string.unquoted.argument.shell,storage.type.object,storage.type.java,constant.other.option",
            "settings": {
                "foreground": "#c6c6c6"
            }
        },
		{
			"scope": "entity.name.function,support.function,support.type.property-name.json,keyword.control.twig",
			"settings": {
				"foreground": "#d4bbff",
			}
		},
        {
            "scope": [
                "meta.embedded",
                "source.groovy.embedded",
                "string meta.image.inline.markdown",
                "variable.legacy.builtin.python"
            ],
            "settings": {
                "foreground": "#c6c6c6"
            }
        },
        {
            "scope": "emphasis",
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "scope": "strong",
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "name": "Comment",
            "scope": "comment",
            "settings": {
                "foreground": "#8d8d8d"
            }
        },
        {
            "name": "block comment",
            "scope": "comment.block",
            "settings": {
                "foreground": "#8d8d8d",
            }
        },
        {
            "scope": "constant.language",
            "settings": {
                "foreground": "#78a9ff"
            }
        },
        {
            "scope": [
                "constant.numeric",
                "variable.other.enummember",
                "keyword.operator.plus.exponent",
                "keyword.operator.minus.exponent"
            ],
            "settings": {
                "foreground": "#3ddbd9"
            }
        },
        {
            "scope": "constant.regexp",
            "settings": {
                "foreground": "#4589ff"
            }
        },
        {
            "scope": "entity.name.tag",
            "settings": {
                "foreground": "#78a9ff"
            }
        },
        {
            "scope": [
                "entity.name.tag.css",
                "entity.name.tag.less"
            ],
            "settings": {
                "foreground": "#78a9ff"
            }
        },
        {
            "scope": "entity.other.attribute-name",
            "settings": {
                "foreground": "#d2a106"
            }
        },
        {
            "scope": [
                "entity.other.attribute-name.class.css",
                "source.css entity.other.attribute-name.class",
                "entity.other.attribute-name.id.css",
                "entity.other.attribute-name.parent-selector.css",
                "entity.other.attribute-name.parent.less",
                "source.css entity.other.attribute-name.pseudo-class",
                "entity.other.attribute-name.pseudo-element.css",
                "source.css.less entity.other.attribute-name.id",
                "entity.other.attribute-name.scss"
            ],
            "settings": {
                "foreground": "#78a9ff"
            }
        },
        {
            "scope": "invalid",
            "settings": {
                "foreground": "#db4934"
            }
        },
        {
            "scope": "markup.underline",
            "settings": {
                "fontStyle": "underline"
            }
        },
        {
            "scope": "markup.bold",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#78a9ff"
            }
        },
        {
            "scope": "markup.heading",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#78a9ff"
            }
        },
        {
            "scope": "markup.italic",
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "scope": "markup.strikethrough",
            "settings": {
                "fontStyle": "strikethrough"
            }
        },
        {
            "scope": "markup.inserted",
            "settings": {
                "foreground": "#d4bbff"
            }
        },
        {
            "scope": "markup.deleted",
            "settings": {
                "foreground": "#6fdc8c"
            }
        },
        {
            "scope": "markup.changed",
            "settings": {
                "foreground": "#78a9ff"
            }
        },
        {
            "scope": "punctuation.definition.quote.begin.markdown",
            "settings": {
                "foreground": "#6fdc8c"
            }
        },
        {
            "scope": "punctuation.definition.list.begin.markdown",
            "settings": {
                "foreground": "#78a9ff"
            }
        },
        {
            "scope": "markup.inline.raw",
            "settings": {
                "foreground": "#6fdc8c"
            }
        },
        {
            "name": "brackets of XML/HTML tags",
            "scope": "punctuation.definition.tag",
            "settings": {
                "foreground": "#a8a8a8"
            }
        },
        {
            "scope": [
                "meta.preprocessor",
                "entity.name.function.preprocessor"
            ],
            "settings": {
                "foreground": "#78a9ff"
            }
        },
        {
            "scope": "meta.preprocessor.string",
            "settings": {
                "foreground": "#6fdc8c"
            }
        },
        {
            "scope": "meta.preprocessor.numeric",
            "settings": {
                "foreground": "#3ddbd9"
            }
        },
        {
            "scope": "meta.diff.header",
            "settings": {
                "foreground": "#78a9ff"
            }
        },
        {
            "scope": "storage",
            "settings": {
                "foreground": "#78a9ff"
            }
        },
        {
            "scope": "storage.type",
            "settings": {
                "foreground": "#78a9ff"
            }
        },
        {
            "scope": [
                "storage.modifier",
                "keyword.operator.noexcept"
            ],
            "settings": {
                "foreground": "#78a9ff"
            }
        },
        {
            "scope": [
                "string",
                "meta.embedded.assembly",
            ],
            "settings": {
                "foreground": "#6fdc8c"
            }
        },
        {
            "scope": "string.tag",
            "settings": {
                "foreground": "#6fdc8c"
            }
        },
        {
            "scope": "string.value",
            "settings": {
                "foreground": "#6fdc8c"
            }
        },
        {
            "scope": "string.regexp",
            "settings": {
                "foreground": "#6fdc8c"
            }
        },
        {
            "name": "String interpolation",
            "scope": [
                "punctuation.definition.template-expression.begin",
                "punctuation.definition.template-expression.end",
                "punctuation.section.embedded"
            ],
            "settings": {
                "foreground": "#78a9ff"
            }
        },
        {
            "name": "Reset JavaScript string interpolation expression",
            "scope": [
                "meta.template.expression"
            ],
            "settings": {
                "foreground": "#c6c6c6"
            }
        },
        {
            "scope": [
                "support.type.vendored.property-name",
                "support.type.property-name",
                "source.css variable",
                "source.coffee.embedded"
            ],
            "settings": {
                "foreground": "#c6c6c6"
            }
        },
        {
            "scope": "keyword",
            "settings": {
                "foreground": "#78a9ff"
            }
        },
        {
            "scope": "keyword.control",
            "settings": {
                "foreground": "#78a9ff"
            }
        },
        {
            "scope": "keyword.operator",
            "settings": {
                "foreground": "#c6c6c6"
            }
        },
        {
            "scope": [
                "keyword.operator.new",
                "keyword.operator.expression",
                "keyword.operator.cast",
                "keyword.operator.sizeof",
                "keyword.operator.alignof",
                "keyword.operator.typeid",
                "keyword.operator.alignas",
                "keyword.operator.instanceof",
                "keyword.operator.logical.python",
                "keyword.operator.wordlike"
            ],
            "settings": {
                "foreground": "#78a9ff"
            }
        },
        {
            "scope": "keyword.other.unit",
            "settings": {
                "foreground": "#3ddbd9"
            }
        },
        {
            "scope": [
                "punctuation.section.embedded.begin.php",
                "punctuation.section.embedded.end.php"
            ],
            "settings": {
                "foreground": "#78a9ff"
            }
        },
        {
            "name": "coloring of the Java import and package identifiers",
            "scope": [
                "storage.modifier.import.java",
                "variable.language.wildcard.java",
                "storage.modifier.package.java"
            ],
            "settings": {
                "foreground": "#c6c6c6"
            }
        },
        {
            "name": "this.self",
            "scope": "variable.language",
            "settings": {
                "foreground": "#78a9ff"
            }
        },
        {
            "name": "built in properties",
            "scope": [
                "support.constant.property-value",
                "keyword.control.at-rule",
            ],
            "settings": {
                "foreground": "#d2a106"
            }
        }
    ],
    "semanticHighlighting": true,
    "semanticTokenColors": {
        "newOperator": "#c6c6c6",
        "stringLiteral": "#6fdc8c",
        "customLiteral": "#c6c6c6",
        "numberLiteral": "#3ddbd9"
    }
}
